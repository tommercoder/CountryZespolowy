<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".phone">
    <include
        android:id="@+id/action_bar"
        layout="@layout/action_bar"
        tools:layout_editor_absoluteY="27dp">

    </include>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/action_bar"
        tools:context=".phone">

        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:fontFamily="@font/bgb"
            android:padding="8dp"
            android:text="Feedback"
            android:textAllCaps="true"
            android:textSize="36sp"
            app:layout_constraintStart_toStartOf="parent"

            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteY="-3dp" />

        <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="175dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="367dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView12">


        a<RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="162dp"
            android:background="@drawable/background_border_choosing_el">

            <!--<TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/available_city"
                android:padding="15dp"
                android:fontFamily="@font/baloo"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                />-->


            <Button
                android:id="@+id/phone_now"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="19dp"
                android:layout_marginTop="86dp"
                android:layout_marginBottom="5dp"
                android:background="#00000000"
                android:padding="20dp"
                android:text="Phone new"
                android:textColor="@color/colorPrimary" />

            <TextView
                android:layout_width="270dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/baloo"
                android:padding="20dp"
                android:text="Asking about app or voting: +380 980 707 638"
                android:textAlignment="center"
                android:textSize="20sp" />

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="99dp"
                android:layout_height="118dp"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="2dp"
                app:srcCompat="@drawable/callus_icon"
                tools:ignore="VectorDrawableCompat" />

        </RelativeLayout>

    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>